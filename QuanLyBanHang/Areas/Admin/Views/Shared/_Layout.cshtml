<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/more.css" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="https://cdn.datatables.net/v/bs/jqc-1.12.4/dt-1.13.4/r-2.4.1/sc-2.1.1/sb-1.4.2/sp-2.1.2/sl-1.6.2/datatables.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
 
</head>
<body style="margin-top:0;padding-top:10px" >
    <div class="container" style="background-color: white; padding-bottom: 10px;">
        <a class="" href="~/Admin/HomeAdmin/Index">
            <img src="~/Upload/MyWeb/Logo-KiotViet.png" style="height:auto;width:100px" alt="Logo" class="logo-image">
        </a>
        <a class="" style="margin-left: 250px; margin-top: 10px;text-decoration:none" href="~/Admin/HomeAdmin/Chude">
            <i class="bi bi-bookmarks-fill"></i>
            Chủ đề
        </a>
        <a style="margin-left: 50px; margin-top: 10px; text-decoration:none" href="~/Admin/HomeAdmin/HoTro">
            <i class="bi bi-flag-fill"></i>  Hỗ trợ
        </a>
        <a style="margin-left: 50px; margin-top: 10px;text-decoration:none" href="https://www.teamviewer.com/apac/download/windows/?utm_source=google&utm_medium=cpc&utm_campaign=asean%7Cb%7Cpr%7C22%7Csep%7Ctv-core-download-sn%7Cnew%7Ct0%7C0&utm_content=Download&utm_term=download+teamviewer&gad=1&gclid=EAIaIQobChMImdWd-PCX_wIVA5yWCh2KWw5vEAAYASAAEgKy5vD_BwE" target="_blank">
            <i class="bi bi-hand-index-thumb-fill"></i>  Tải Teamview
        </a>
        @if (Session["USER_SESSION"] != null)
        {
            <a class="pull-right" href="~/Admin/Login/SignOut" style="color: black !important;margin-right: 20px;">Đăng xuất</a>
        }
       
        </div>


    <div class="navbar navbar-inverse " style="background-color: deepskyblue; border: none;">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="~/Admin/HomeAdmin/Index" style="color: white !important;"><i class="bi bi-eye" style="padding-right:10px"></i>Tổng quan</a>
                    </li>
                    <li>
                        <a  href="~/Admin/Home/Contact" class="dropdown-toggle" data-toggle="dropdown" style="color: white !important;">
                            <i class="bi bi-box-fill"></i>
                           Hàng hóa
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="~/Admin/HangHoa/Index">Danh mục</a></li>
                            <li><a href="~/Admin/KiemKho/Index">Kiểm kho</a></li>
                        </ul>
                    
                    </li>
                  
                    <li style="color: white !important;">
                        <a href="~/Admin/Home/Contact" class="dropdown-toggle" data-toggle="dropdown"  style="color: white !important;">
                            <i class="bi bi-people-fill"></i>
                            Đối tác
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="~/Admin/KhachHang/Index
                                   ">Khách hàng</a></li>
                            <li><a href="#">Nhà cung cấp</a></li>
                         
                        </ul>
    
                    </li>
                    <li style="color: white !important;">
                        <a href="~/Admin/Home/Contact" class="dropdown-toggle" data-toggle="dropdown" style="color: white !important;">
                            <i class="bi bi-receipt"></i>
                            Hóa đơn
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="~/Admin/Invoice/UnFinished">Đang chờ duyệt</a></li>
                            <li><a href="~/Admin/Invoice/Finished">Đã bán</a></li>
                       
                        </ul>
                      
                    </li>
                  
                 
                </ul>
                    <ul class="nav navbar-nav navbar-right">
                       
                        <li>
                            <a href="~/Admin/BanHang/Index"style="background-color:dodgerblue;color:white!important">    
                                    <i class="bi bi-cart2"></i>
                                    Bán hàng
                              
                            </a>
                        </li>
                    </ul>
                
            </div>
        </div>
    </div>



    <div class="">
        <script src="~/Scripts/JSTB.js"></script>
        <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
        <script src="https://cdn.datatables.net/v/bs/jqc-1.12.4/dt-1.13.4/r-2.4.1/sc-2.1.1/sb-1.4.2/sp-2.1.2/sl-1.6.2/datatables.min.js"></script>

        @*//cdn.datatables.net/1.10.2/js/jquery.dataTables.js
        //cdn.datatables.net/1.10.2/js/jquery.dataTables.min.js*@
        @RenderBody()
   
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
